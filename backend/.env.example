# Server Configuration
PORT=5000
NODE_ENV=development

# MongoDB Configuration
MONGO_URI=your_mongodb_connection_string_here

# Google Gemini API Configuration (Primary AI Provider)
GEMINI_API_KEY=your_gemini_api_key_here

# Groq API Configuration (Fallback AI Provider)
GROQ_API_KEY=your_primary_groq_api_key_here
GROQ_FALLBACK_API_KEY=your_fallback_groq_api_key_here

# Firebase Admin SDK
FIREBASE_SERVICE_ACCOUNT_PATH=./firebase-admin-sdk.json
RTDB_URL=your_firebase_realtime_database_url_here

# CORS
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:5174,http://localhost:3000
